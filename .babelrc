{
  "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/preset-react"],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@components": "./src/components",
          "@styles": "./src/styles",
          "@hooks": "./src/hooks",
          "@utils": "./src/utils",
          "@consts": "./src/consts",
          "@types": "./src/consts"
        }
      }
    ]
  ]
}
